using System::Console;

func calcC(n : int, m : int) : void {
    var C = $int[n][n + 1];
    for (var i = 0; i < n; i++) C[i][0] = 1;
    for (var i = 1; i < n; i++)
        for (var j = 1; j <= i; j++)
            C[i][j] = (C[i - 1][j] + C[i - 1][j - 1]) % m;
    // for (var i = 0; i < n; i++) {
    //     for (var j = 0; j <= i; j++) print(C[i][j]), putchar(' ');
    //     putchar('\n');
    // }
}