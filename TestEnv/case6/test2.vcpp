using System::Console;
using System::Algorithm;
using System;

public class Point {
    public var : int x, y;
    public func @constructer(x : int, y : int) : void {
        @this.x = x, @this.y = y;
    }
    public func output() : void {
        print(x), putchar(' '), print(y), putchar('\n');
    }
}

public class PointAdd : Add<$Point, Point, Point$> {
    public func @constructer() : void { @base(); }
    public varfunc calculate(a : Point, b : Point) : Point {
        return $Point(a.x + b.x, a.y + b.y);
    }
}

var @add : PointAdd;

public func testVectorCalc() : void {
    initRand(getTime());
    var pls = $Point[105];
    @add = $PointAdd();
    for (var i = 0; i < 100; i++) pls[i] = $Point(getRand(0, 100), getRand(0, 100));
    for (var i = 1; i < 100; i++) {
        pls[i - 1].output(), print("+\n");
        pls[i].output(), print("=\n");
        (pls[i - 1] + pls[i]).output();
    }
}